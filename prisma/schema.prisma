// prisma/schema.prisma

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

// Define the Prisma schema
model products {
  product_id              String          @id
  availability            String?
  name                    String?
  price                   String?
  oldPrice                String?
  displayDiscountPercentage String?
  familyName              String?
  subfamilyName           String?
  sectionName             String?
  img                     String?
  link                    String?
}


